{
  "name": "cosroom",
  "version": "1.0.0",
  "description": "Find a room at COS",
  "author": "Steven Loria",
  "repository": "sloria/cosroom",
  "scripts": {
    "test": "npm run lint && jest",
    "build": "NODE_ENV=production webpack --progress --colors -p",
    "dev": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask\"",
    "webpack-dev-server": "NODE_ENV=debug webpack-dev-server --port 2992 --hot --inline",
    "flask": "FLASK_APP=web/app.py FLASK_DEBUG=1 flask run",
    "lint": "eslint \"web/static/*.js\"",
    "heroku-postbuild": "npm run build",
    "deploy": "npm test && git push heroku master"
  },
  "license": "MIT",
  "dependencies": {
    "@trainline/react-skeletor": "^1.0.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.0.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "date-fns": "^1.28.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "manifest-revision-webpack-plugin": "^0.4.0",
    "microfeedback-button": "^0.3.0",
    "nprogress": "^0.2.0",
    "preact": "^8.2.5",
    "preact-compat": "^3.17.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.19.0",
    "sync-exec": "^0.6.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1"
  },
  "engines": {
    "node": "8.9.x",
    "npm": "5.4.x"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-jest": "^22.0.0",
    "concurrently": "^3.5.0",
    "eslint": "^4.11.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-react": "^7.4.0",
    "jest": "^21.2.1",
    "webpack-dev-server": "^2.4.5"
  },
  "jest": {
    "moduleNameMapper": {
      "^react$": "preact-compat",
      "^react-dom$": "preact-compat"
    }
  }
}
