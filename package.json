{
  "name": "cosroom",
  "version": "1.0.0",
  "description": "Find a room at COS",
  "author": "Steven Loria",
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --colors -p",
    "start": "concurrently -n \"WEBPACK,FLASK\" -c \"bgBlue.bold,bgMagenta.bold\" \"npm run webpack-dev-server\" \"npm run flask-server\"",
    "webpack-dev-server": "NODE_ENV=debug webpack-dev-server --port 2992 --hot --inline",
    "flask-server": "FLASK_APP=web/app.py FLASK_DEBUG=1 flask run",
    "lint": "eslint \"web/static/*.js\"",
    "heroku-postbuild": "npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "date-fns": "^1.28.5",
    "vue": "^2.4.4",
    "webpack": "^2.6.1",
    "css-loader": "^0.28.4",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.11.2",
    "manifest-revision-webpack-plugin": "^0.4.0",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "sync-exec": "^0.6.2",
    "url-loader": "^0.5.9"
  },
  "engines": {
    "node": "8.1.x",
    "npm": "5.4.x"
  },
  "devDependencies": {
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "webpack-dev-server": "^2.4.5"
  }
}
